{
    "parser": "espree",
    "parserOptions": {
        "sourceType": "module"
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "globals": {},
    "rules": {
        "quotes": [
            1,
            "single", {
                "avoidEscape": true
            }
        ],
        "no-extra-parens": 1,
        "curly": 1,
        "eqeqeq": 1,
        "no-else-return": 1,
        "no-eval": 1,
        "no-implicit-coercion": 1,
        "no-labels": 2,
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-multi-spaces": 2,
        "no-multi-str": 1,
        "no-native-reassign": [
            2,
            {
                "exceptions": [
                    "Object"
                ]
            }
        ],
        "no-new-wrappers": 1,
        "no-new": 2,
        "no-param-reassign": 2,
        "no-redeclare": 2,
        "no-return-assign": 1,
        "no-self-compare": 2,
        "no-shadow-restricted-names": 2,
        "no-shadow": 1,
        "no-undef-init": 1,
        "no-undefined": 2,
        "brace-style": [
            1,
            "1tbs",
            {
                "allowSingleLine": false
            }
        ],
        "camelcase": 1,
        "comma-spacing": [
            2,
            {
                "before": false,
                "after": true
            }
        ],
        "comma-style": [
            1,
            "last"
        ],
        "indent": 1,
        "new-cap": 1,
        "new-parens": 2,
        "no-array-constructor": 1,
        "no-continue": 1,
        "no-inline-comments": 1,
        "no-lonely-if": 1,
        "no-mixed-spaces-and-tabs": 2,
        "no-new-object": 1,
        "no-spaced-func": 1,
        "no-trailing-spaces": 1,
        "no-unneeded-ternary": 2,
        "semi": [
            2,
            "always"
        ],
        "space-before-blocks": 1,
        "space-before-function-paren": [
            2,
            {
                "anonymous": "always",
                "named": "never"
            }
        ],
        "space-infix-ops": 1,
        "space-unary-ops": 1,
        "spaced-comment": 1,
        "no-unused-vars": 2,
        "react/prop-types": 0
    }
}
